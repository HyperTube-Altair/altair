{
	"extends": "next/core-web-vitals",
	"plugins": ["import"],
	"rules": {
		"no-console": 1,
		"no-unused-vars": 2,
		"no-dupe-else-if": 2,
		"no-self-assign": 2,
		"import/no-unused-modules": [2, { "unusedExports": true }]
	},
	"overrides": [
		{
			"files": ["page.tsx", "layout.tsx", "store.ts"],
			"rules": {
				"import/no-unused-modules": "off"
			}
		}
	]
}
